{
  "name": "uni-vue3-router",
  "version": "1.0.0",
  "description": "A Vue Router-style routing library for uni-app, supporting vite+vue3+javascript/typescript environments. Vue Router风格的uni-app路由库，支持vite+vue3+javascript/typescript环境",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "npx rollup -c -w",
    "build": "npx rollup -c",
    "prepublishOnly": "npm run build",
    "lint": "npx eslint src --ext .js,.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "uni-app",
    "router",
    "vue3",
    "vite",
    "navigation",
    "typescript"
  ],
  "author": "wangmiaozero",
  "license": "Custom",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangmiaozero/uni-vue3-router.git"
  },
  "bugs": {
    "url": "https://github.com/wangmiaozero/uni-vue3-router/issues"
  },
  "homepage": "https://github.com/wangmiaozero/uni-vue3-router#readme",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@dcloudio/types": "^3.0.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.19.0",
    "rollup": "^2.77.0",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
} 